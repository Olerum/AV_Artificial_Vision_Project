class Object:
    def __init__(self, name, pos=(0,0)):
        self.name = name
        self.pos = pos
        self.inc_pos = (10,5)
        
    def next_pos(self):
        self.pos = ( self.pos[0]+self.inc_pos[0], self.pos[1]+self.inc_pos[1] )


obj = Object('car')


for t in range(10):
    print('Object pos of', obj.name, 'at', t, obj.pos)
    obj.next_pos()








